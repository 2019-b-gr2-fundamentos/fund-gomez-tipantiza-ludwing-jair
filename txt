#include <iostream>
#include <stdlib.h>
// Datos Primitivos
using namespace std;


int main()
{
	char    letra = 'c';  // Solo un caracter
	cout << letra <<"\n \n" << endl;
	
	short int numero= 85; // Numero entero
	cout << numero <<"\n \n" << endl;
	
	int edad = 19;       // Numero entero
	cout << edad <<"\n \n" << endl;
	
	long int cantidad = 40000;            //Numero entero
	cout << cantidad <<"\n \n" << endl;
	
	bool loco = true;    //Almacena dos valores
	cout << loco <<"\n \n" << endl;
	
	double division = 975.65; //Almacena numeros con parte decimal
	cout << division <<"\n \n" << endl;
	

    
#include <stdio.h>
#include <stdlib.h>


int main()
{
	printf("Hola Mundo \n \n");
	system("PAUSE");
	return 0;
}